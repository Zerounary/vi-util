{
  "name": "vi-util",
  "version": "1.0.0",
  "description": "string utils",
  "main": "dist/index.js",
  "scripts": {
    "test": "nyc --reporter=html --reporter=lcov --reporter=text mocha --require babel-register",
    "test:watch": "mocha --require babel-register",
    "prebuild": "rimraf dist",
    "build": "rollup -c ",
    "build:webpack": "webpack",
    "check-coverage": "nyc check-coverage",
    "semantic-release": "semantic-release --branches main",
    "commit": "git-cz -a"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Zerounary/vi-util.git"
  },
  "keywords": [
    "string",
    "utils"
  ],
  "files": [
    "dist",
    "README.md"
  ],
  "author": "Zerounary",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Zerounary/vi-util/issues"
  },
  "homepage": "https://github.com/Zerounary/vi-util#readme",
  "devDependencies": {
    "babel-loader": "^7.1.5",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.3.6",
    "commitizen": "^4.2.4",
    "coveralls": "^3.1.1",
    "cz-conventional-changelog": "^3.0.1",
    "ghooks": "^2.0.4",
    "istanbul": "^0.4.5",
    "json-loader": "^0.5.7",
    "mocha": "^9.2.1",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.68.0",
    "rollup-plugin-terser": "^7.0.2",
    "semantic-release": "^19.0.2",
    "webpack": "^5.69.1",
    "webpack-cli": "^4.9.2"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run test && npm run check-coverage && npm run build"
    }
  }
}
