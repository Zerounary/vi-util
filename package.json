{
  "name": "vi-util",
  "version": "1.0.0",
  "description": "string utils",
  "main": "src/index.js",
  "scripts": {
    "test": "nyc --reporter=html --reporter=lcov --reporter=text-summary mocha",
    "check-coverage": "nyc check-coverage",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "semantic-release": "semantic-release --branches main",
    "commit": "git-cz -a"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Zerounary/vi-util.git"
  },
  "keywords": [
    "string",
    "utils"
  ],
  "author": "Zerounary",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Zerounary/vi-util/issues"
  },
  "homepage": "https://github.com/Zerounary/vi-util#readme",
  "devDependencies": {
    "chai": "^4.3.6",
    "codecov.io": "^0.1.6",
    "commitizen": "^4.2.4",
    "coveralls": "^3.1.1",
    "cz-conventional-changelog": "^3.3.0",
    "ghooks": "^2.0.4",
    "istanbul": "^0.4.5",
    "mocha": "^9.2.1",
    "nyc": "^15.1.0",
    "semantic-release": "^19.0.2"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run test && npm run check-coverage"
    }
  }
}
